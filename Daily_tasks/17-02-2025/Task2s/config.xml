<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="loanBean" class="examples.LoanBean">
		<property name="loanAmount"> 
			<value>1000000</value>
		</property>
		<property name="loanInterest">
			<value>5.2</value>
		</property>
		<property name="tenure">
			<value>13</value>
		</property>
	</bean>
</beans>
